
public class InnerClass {
        private int a=10;
        class Inner
        {
        	void show()
        	{
        		System.out.println("value of A is= "+a);
        	}
        }
	public static void main(String[] args) {
		InnerClass obj = new InnerClass();
		InnerClass.Inner in = obj.new Inner();
		in.show();

	}

}
